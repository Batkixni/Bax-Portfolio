    "front-matter": "^4.0.2",
    "fs-extra": "^11.1.1",
    "gsap": "^3.13.0",
    "htmx.org": "^2.0.6",
    "marked": "^9.1.6",
    "node-fetch": "^3.3.2",
    "p5": "^2.0.3",
    "sharp": "^0.34.3",
    "vidstack": "^0.6.15"
